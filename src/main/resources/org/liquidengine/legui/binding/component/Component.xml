<class-binding default="true" for="org.liquidengine.legui.component.Component" to="component" xmlns="binding.legui.liquidengine.org">
  <bind field="position" to="position">
    <using-binding path="org/liquidengine/legui/binding/common/Vector2f.xml"/>
  </bind>
  <bind field="size" to="size">
    <using-binding path="org/liquidengine/legui/binding/common/Vector2size.xml"/>
  </bind>
  <bind field="enabled"/>
  <bind field="focused"/>
  <bind field="pressed"/>
  <bind field="tabIndex" to="tabIndex"/>
  <bind field="childs" as="field" to="childs">
    <using-accessor class="prg.liquidengine.legui.binding.accessor.ComponentChildsAccessor"/>
  </bind>
  <!--<bind field="childs" getter="getChilds" setter="addAll" as="field" to="childs"/>-->
  <!--<unbind field="components"/>-->
</class-binding>